<p id="notice"><%= notice %></p>

<h1>Daily In Counts</h1>

<p>
<%= link_to 'New Daily In Count', new_daily_in_count_path %>
</p>

<p>
<%= page_entries_info @daily_in_counts %>
</p>

<table>
  <thead>
    <tr>
      <th>Count date</th>
      <th>Site</th>
      <th>Count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_in_counts.each do |daily_in_count| %>
      <tr>
        <td><%= dt = daily_in_count.count_date; sprintf("%02d-%02d-%02d",dt/10000,dt/100%100,dt%100) %></td>
        <td><%= image_tag daily_in_count.site.icon_url %><%= daily_in_count.site.name %></td>
        <td><%= daily_in_count.count.to_s(:delimited) %></td>
        <td><%= link_to 'Show', daily_in_count %></td>
        <td><%= link_to 'Edit', edit_daily_in_count_path(daily_in_count) %></td>
        <td><%= link_to 'Destroy', daily_in_count, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<p>
<%= paginate @daily_in_counts %>
</p>
