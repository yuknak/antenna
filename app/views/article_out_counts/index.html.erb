<p id="notice"><%= notice %></p>

<h1>Article Out Counts</h1>

<table>
  <thead>
    <tr>
      <th>Last time</th>
      <th>Article</th>
      <th>Count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @article_out_counts.each do |article_out_count| %>
      <tr>
        <td><%= article_out_count.last_time %></td>
        <td><%= article_out_count.article_id %></td>
        <td><%= article_out_count.count %></td>
        <td><%= link_to 'Show', article_out_count %></td>
        <td><%= link_to 'Edit', edit_article_out_count_path(article_out_count) %></td>
        <td><%= link_to 'Destroy', article_out_count, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Article Out Count', new_article_out_count_path %>
