<p id="notice"><%= notice %></p>

<h1>In Histories</h1>

<p>
<%= link_to 'New In History', new_in_history_path %>
</p>

<%= page_entries_info @in_histories %>

<table>
  <thead>
    <tr>
      <th>Rec time</th>
      <th>Site</th>
      <th>Referer</th>
      <th>Ip</th>
      <th>Chkd</th>
      <th>Request line</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @in_histories.each do |in_history| %>
      <tr>
        <td><%= in_history.rec_time %></td>
        <td><%= in_history.site_id %></td>
        <td><%= in_history.referer %></td>
        <td><%= in_history.ip %></td>
        <td><%= in_history.chkd %></td>
        <td><%= in_history.request_line %></td>
        <td><%= link_to 'Show', in_history %></td>
        <td><%= link_to 'Edit', edit_in_history_path(in_history) %></td>
        <td><%= link_to 'Destroy', in_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @in_histories %>
